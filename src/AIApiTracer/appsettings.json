{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "openai-route": {
        "ClusterId": "openai-cluster",
        "Match": {
          "Path": "/endpoint/openai/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catch-all}"
          }
        ]
      },
      "anthropic-route": {
        "ClusterId": "anthropic-cluster",
        "Match": {
          "Path": "/endpoint/anthropic/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catch-all}"
          }
        ]
      },
      "xai-route": {
        "ClusterId": "xai-cluster",
        "Match": {
          "Path": "/endpoint/x/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catch-all}"
          }
        ]
      },
      "azure-openai-route": {
        "ClusterId": "azure-openai-cluster",
        "Match": {
          "Path": "/endpoint/aoai/{**catch-all}"
        }
      },
      "openai-compat-route": {
        "ClusterId": "openai-compat-cluster",
        "Match": {
          "Path": "/endpoint/openai-compat/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "openai-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://api.openai.com/"
          }
        },
        "HttpClient": {
          "EnableMultipleHttp2Connections": true
        },
        "HttpRequest": {
          "AllowResponseBuffering": false
        }
      },
      "anthropic-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://api.anthropic.com/"
          }
        },
        "HttpClient": {
          "EnableMultipleHttp2Connections": true
        },
        "HttpRequest": {
          "AllowResponseBuffering": false
        }
      },
      "xai-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://api.x.ai/"
          }
        },
        "HttpClient": {
          "EnableMultipleHttp2Connections": true
        },
        "HttpRequest": {
          "AllowResponseBuffering": false
        }
      },
      "azure-openai-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://dummy.openai.azure.com/"
          }
        },
        "HttpClient": {
          "EnableMultipleHttp2Connections": true
        },
        "HttpRequest": {
          "AllowResponseBuffering": false
        }
      },
      "openai-compat-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://dummy.example.com/"
          }
        },
        "HttpClient": {
          "EnableMultipleHttp2Connections": true
        },
        "HttpRequest": {
          "AllowResponseBuffering": false
        }
      }
    }
  }
}
